    "@react-navigation/stack": "^7.1.1",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "expo": "~52.0.20",
    "expo-battery": "~9.0.1",
    "expo-intent-launcher": "~12.0.1",
    "expo-modules-core": "^2.1.2",
    "expo-notifications": "^0.29.11",
    "expo-secure-store": "^14.0.0",
