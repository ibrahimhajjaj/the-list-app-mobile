{
  "name": "expo-frontend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "export:android": "expo prebuild -p android && cd android && ./gradlew assembleRelease",
